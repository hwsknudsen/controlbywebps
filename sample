$username = "user"
$password = "pw"
$base64AuthInfo = [Convert]::ToBase64String([Text.Encoding]::ASCII.GetBytes(("{0}:{1}" -f $username,$password)))

$response = Invoke-RestMethod -Headers @{Authorization=("Basic {0}" -f $base64AuthInfo)} -Uri "http://address/customState.json?showUnits=0&showColors=0"
$response.'dieselVolume(gallons)'
